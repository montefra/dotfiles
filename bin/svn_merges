#!/bin/bash
# loop through all the branches and runs
# svn mergeinfo $branch
# svn mergeinfo --show-revs eligible $branch

branches='^/branches'

for b in `svn ls $branches`
do
    branch=$branches/$b
    echo "==== $branch ===="
    svn mergeinfo $branch
    echo eligible: `svn mergeinfo --show-revs eligible $branch`
    echo merged: `svn mergeinfo --show-revs merged $branch`
    echo "xxxx $branch xxxx"
done
